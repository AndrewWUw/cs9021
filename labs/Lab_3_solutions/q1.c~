#include <stdio.h>
#include <stdlib.h>
#include <ctype.h>

bool isPrime(int num);

int main(void) {
    return EXIT_SUCCESS;
}

bool isPrime(int num) {
    bool is_prime = true;
    for (int k = 3; k <= sqrt(num); k += 2){
        if (num % k == 0) {
            is_prime = false;
            break;
        }
    }
}


